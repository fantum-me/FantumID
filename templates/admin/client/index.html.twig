{% extends '/base.html.twig' %}

{% block title %}OAuthClient index{% endblock %}

{% block body %}
    <h1>OAuthClient index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Secret</th>
                <th>Id</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for o_auth_client in o_auth_clients %}
            <tr>
                <td>{{ o_auth_client.secret }}</td>
                <td>{{ o_auth_client.id }}</td>
                <td>
                    <a href="{{ path('app_client_show', {'id': o_auth_client.id}) }}">show</a>
                    <a href="{{ path('app_client_edit', {'id': o_auth_client.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_client_new') }}">Create new</a>
{% endblock %}
