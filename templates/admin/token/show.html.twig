{% extends 'base.html.twig' %}

{% block title %}AccessToken{% endblock %}

{% block body %}
    <h1>AccessToken</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ access_token.id }}</td>
            </tr>
            <tr>
                <th>Token</th>
                <td>{{ access_token.token }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_admin_token_index') }}">back to list</a>

    <a href="{{ path('app_admin_token_edit', {'id': access_token.id}) }}">edit</a>

    {{ include('admin/token/_delete_form.html.twig') }}
{% endblock %}
